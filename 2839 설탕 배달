#include <cstdio>
int main(){
    int n; ///설탕의 양
    scanf("%d",&n);
    int t=0; ///3kg 설탕봉지의 수
    while(1){
        if(n%5==0){
            printf("%d\n",n/5+t);
            break;
        }else if(n<=0){
            printf("-1\n");
            break;
        }
        n-=3;
        t++;
    }
    return 0;
}
